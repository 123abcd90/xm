function showChooseTips(){if("/user/borrow"!=location.pathname&&window.localStorage.waiteChooseRenewTenderNum>0){var e=".newVersion .set-renew-tender-tip{display:none;position:fixed;top:0;bottom:0;left:0;right:0;z-index:1000;text-align:center}.newVersion .set-renew-tender-tip .tips-background{position:absolute;top:0;right:0;bottom:0;left:0;background:#000;opacity:.63;filter:alpha(opacity=63);z-index:999}.newVersion .set-renew-tender-tip .tips-helper{display:inline-block;vertical-align:middle;width:1px;height:100%;line-height:100%;background:transparent}.newVersion .set-renew-tender-tip .tips{position:relative;display:inline-block;vertical-align:middle;width:450px;height:440px;background-color:#fff;border-radius:10px;z-index:10000;background-image:url(https://img3-itzcdn-com.alikunlun.com/static_res/img/common/tender_tips_eba79f9.png?__sprite);background-repeat:no-repeat;background-position:center 45px}.newVersion .set-renew-tender-tip .tips-body{position:absolute;bottom:0;left:108px;right:108px}.newVersion .set-renew-tender-tip .tips-body p{font-size:16px;color:#333;line-height:1.5;margin-bottom:15px}.newVersion .set-renew-tender-tip .tips-body p span{color:#e55}.newVersion .set-renew-tender-tip .tips-body a{display:inline-block;margin-bottom:60px}";addStyle2head(e);var t='<div class="set-renew-tender-tip">				<div class="tips-background"></div>				<div class="tips-helper"></div>				<div class="tips">					<div class="tips-body">						<p>							您有<span>'+window.localStorage.waiteChooseRenewTenderNum+'</span>个待选择同意展期的项目，请尽快完成选择操作！						</p>						<a href="/user/borrow" onClick="window.localStorage.waiteChooseRenewTenderNum = 0;" class="btn btn-red btn-debt-transfer">去选择</a>					</div>				</div>			</div>';$(".set-renew-tender-tip").length<=0&&$("body").append(t);var i=$(".set-renew-tender-tip");i.fadeIn()}}$(function(){showChooseTips()});